{
  "name": "@driveros/mobile",
  "version": "1.0.0",
  "description": "DriverOS Mobile App - React Native with Expo",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:preview": "eas build --profile preview",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "expo": "~50.0.0",
    "expo-router": "~3.4.0",
    "expo-linking": "~6.2.0",
    "expo-constants": "~15.4.0",
    "expo-status-bar": "~1.11.0",
    "expo-secure-store": "~12.8.0",
    "expo-location": "~16.5.0",
    "expo-camera": "~14.0.0",
    "expo-barcode-scanner": "~12.9.0",
    "expo-notifications": "~0.27.0",
    "expo-background-fetch": "~11.8.0",
    "expo-task-manager": "~11.7.0",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tanstack/react-query": "^5.0.0",
    "@driveros/types": "workspace:*",
    "react-hook-form": "^7.48.0",
    "@hookform/resolvers": "^3.3.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0",
    "react-native-svg": "14.1.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-paper": "^5.11.0",
    "react-native-elements": "^3.4.3",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-super-grid": "^4.9.0",
    "react-native-chart-kit": "^6.12.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "typescript": "^5.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "prettier": "^3.2.0",
    "@expo/config-plugins": "~7.8.0",
    "eas-cli": "^5.9.0"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  }
}
